{
  "runtime.version": "LuaJIT",
  "runtime.special": {
    "vim.api.nvim_exec": "loadstring",
    "vim.api.nvim_command": "loadstring",
    "vim.api.nvim_eval": "loadstring"
  },
  "diagnostics.globals": [
    "vim"
  ],
  "workspace.library": [
    "$VIMRUNTIME",
    "${3rd}/luv/library",
    "${3rd}/busted/library",
    "${3rd}/luassert/library"
  ],
  "workspace.checkThirdParty": false,
  "telemetry.enable": false
}
